# This is Git's per-user configuration file.
[user]
	name = frerepoulet
	email = olivier@frerepoulet.com
[core]
	autocrlf = input
	safecrlf = true
	excludesfile = /Users/olivier/.gitignore_global
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
	s = status -s
